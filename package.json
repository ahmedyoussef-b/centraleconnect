{
  "name": "centraleconnect",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmedyoussef-b/centraleconnect.git"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild-check": "node ./prebuild-check.js",
    "build": "npm run prebuild-check && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "check:all": "npm run lint && npm run typecheck",
    "seed": "prisma db push --force-reset && tsx scripts/seed.ts",
    "generate:scada-map": "tsx scripts/generate-scada-mapping.ts",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "models:download": "node scripts/download-vosk-model.js",
    "setup:ai-models": "node scripts/setup-ai-models-node.js",
    "test:ai": "next dev --port 3001",
    "tauri": "tauri"
  },
